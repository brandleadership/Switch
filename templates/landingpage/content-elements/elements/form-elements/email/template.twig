<div data-bsi-element="email" style="padding-bottom: 15px;">
    <div data-bsi-element-part="form-field"
        class="reverse-for-required text-field"
        data-bsi-form-field-fixed-type=""
        id="email-wrapper"
        class="email-wrapper">
        <input id="email"
            name="email"
            class="custom-form-field email"
            type="email"
            spellcheck="true"
            placeholder="Email"
            required="" />
        <div class="error-message isVisible">
        {{properties.email_error}}
        </div>
        <label class="text-field-label" for="email">
        {{properties.labels.email}}
        </label>
    </div>
</div>
