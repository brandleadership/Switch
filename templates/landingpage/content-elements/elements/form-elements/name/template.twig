<div data-bsi-element="name" style="padding-bottom: 15px;">
    <div data-bsi-element-part="form-field"
        class="reverse-for-required text-field"
        data-bsi-form-field-fixed-type=""
        class="first-name-wrapper">
        <input id="first-name"
            name="first-name"
            type="text"
            class="custom-form-field first-name"
            spellcheck="true"
            placeholder="{{properties.labels.first_name}}"
            required=" " />
        <label class="text-field-label" for="first-name">
      {{properties.labels.first_name}}
        </label>
    </div>
</div>
